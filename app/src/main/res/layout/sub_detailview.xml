<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:id="@+id/linparent"
    >
    <RelativeLayout
        android:id="@+id/driver_list_rl_header"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/colorPrimary" >

        <ImageView
            android:id="@+id/driver_list_iv_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:padding="20dp"
            android:layout_centerVertical="true"
            android:src="@drawable/back" />

        <TextView
            android:id="@+id/driver_list_tv_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:layout_toRightOf="@+id/driver_list_iv_back"
            android:text="@string/app_name"
            android:textColor="#fff"
            android:textSize="18sp"
            />
<TextView
    android:id="@+id/t_home"
    android:layout_width="wrap_content"
    android:layout_alignParentRight="true"
    android:layout_height="wrap_content"
    android:textColor="@color/white"
    android:textStyle="bold"
    android:gravity="center"
  android:layout_margin="5dp"
    android:padding="3dp"
    android:textSize="35dp"
    android:text="@string/homefont"></TextView>

    </RelativeLayout>
    <LinearLayout
        android:id="@+id/lintop"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"

        android:orientation="horizontal">


        <TextView
            android:id="@+id/tt2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/max_margin"
            android:layout_marginRight="2dp"
android:layout_marginLeft="2dp"
            android:layout_marginBottom="15dp"
android:visibility="gone"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="Log Report"
            android:textColor="@color/black"
            android:textSize="16dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tt3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/max_margin"
            android:layout_marginLeft="2dp"

            android:layout_marginBottom="15dp"

            android:layout_gravity="center"
            android:gravity="center"
            android:text=" "
            android:textColor="@color/black"
            android:textSize="16dp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/txtdr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/max_margin"
            android:layout_marginLeft="5dp"
            android:layout_marginBottom="15dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:text=" "
            android:textColor="@color/black"
            android:textSize="15dp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/txtcertfy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/text_small"
            android:layout_marginTop="@dimen/medium_padding"

            android:layout_marginBottom="@dimen/medium_padding"
            android:layout_gravity="center|center_horizontal"
            android:gravity="center"
            android:background="@drawable/button_ident"
            android:textColor="@color/white"
            android:text=" CERTIFY NOW "
            android:layout_marginRight="@dimen/min_margin"
            android:textSize="16dp"
            android:textStyle="bold" />


        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:gravity="right"
            android:visibility="gone"
            android:orientation="horizontal">
            <ImageView

                android:id="@+id/imgprint"
                android:layout_width="50dp"
                android:layout_marginRight="@dimen/max_margin"
                android:layout_height="50dp"
                android:layout_gravity="right"
                android:gravity="right"
                android:src="@drawable/printer"
                android:visibility="invisible"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linscd"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="right|center"
        android:gravity="right|center"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="5dp"
        android:orientation="horizontal">
        <!-- <com.isoft.trucksoft_elog.Isoft_activity.MovableFloatingActionButton
             android:id="@+id/fab"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_gravity="top|left"
             android:scaleType="center"
             app:backgroundTint="@color/red"
             android:layout_margin="@dimen/fab_margin"
             android:layout_marginRight="10dp"
             android:src="@android:drawable/ic_dialog_email" />-->
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/rephead_fontport"
            android:textStyle="bold"
            android:padding="8dp"
            android:textColor="@color/companycolor"
            android:text=" ROADSIDE INSPECTION :"></TextView>

        <TextView
            android:id="@+id/txtemail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/rephead_fontport"
            android:textStyle="bold"
            android:layout_marginRight="10dp"
            android:padding="8dp"
            android:textColor="@color/white"
            android:background="@drawable/button_red"
            android:text=" EMAIL "></TextView>


        <TextView
            android:id="@+id/tfmcsa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/rephead_fontport"
            android:textStyle="bold"
            android:layout_marginRight="30dp"
            android:padding="8dp"
            android:textColor="@color/white"
            android:background="@drawable/button_compny"
            android:text="  ELECTRONIC WEB TRANSFER  "></TextView>


    </LinearLayout>
    <View
        android:id="@+id/view1v"
        android:layout_marginTop="5dp"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/blue"></View>
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        >
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <FrameLayout
                android:id="@+id/frame_memecontent"

                android:layout_marginLeft="1dp"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ProgressBar
                    style="?android:attr/progressBarStyleLarge"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:indeterminate="true"
                    android:layout_gravity="center" />

                <WebView
                    android:id="@+id/webview"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:visibility="visible"
                    />
                <ImageView
                    android:id="@+id/myBitmapz"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/graph_margin"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:visibility="gone"

                    />
            </FrameLayout>




            <View
                android:id="@+id/view0"
                android:layout_marginTop="3dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/blue"></View>


            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="#7F7F7F"
                android:orientation="horizontal"
                android:padding="@dimen/low_padding">

                <TableRow
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:singleLine="true"
                        android:layout_weight="0.45"
                        android:text="STATUS"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:textStyle="bold"/>
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textStyle="bold"

                        android:singleLine="true"
                        android:textColor="@color/white"
                        android:text="@string/tallow" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:singleLine="true"
                        android:layout_weight="0.8"
                        android:text="TIME USED"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:textStyle="bold"/>

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textStyle="bold"

                        android:singleLine="true"
                        android:textColor="@color/white"
                        android:text="@string/tleft" />



                </TableRow>
            </TableLayout>

            <View
                android:id="@+id/view1"

                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/blue"></View>


            <LinearLayout
                android:id="@+id/chatactivity_listview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="5dp">
            </LinearLayout>
            <View
                android:id="@+id/view3s"

                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/blue"></View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/txtdate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_weight="1"
                android:textColor="@color/companycolor"/>
            <TextView
                android:id="@+id/txtexce"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_weight="1"
                android:textStyle="bold"
                android:textColor="@color/companycolor"/>
        </LinearLayout>

            <View
                android:id="@+id/view2s"

                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/blue"></View>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="#7F7F7F"
        android:orientation="horizontal"
        android:padding="@dimen/low_padding">

        <TableRow
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_weight="0.4"
                android:text="STATUS"
                android:gravity="center"
                android:textColor="@color/white"
                android:textStyle="bold"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_weight="0.4"
                android:text="START"
                android:gravity="center"
                android:textColor="@color/white"
                android:textStyle="bold"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_weight="0.4"
                android:text="STOP"
                android:gravity="center"
                android:textColor="@color/white"
                android:textStyle="bold"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_weight="0.4"
                android:text="DURATION"
                android:gravity="center"
                android:textColor="@color/white"
                android:textStyle="bold"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:textStyle="bold"
                android:layout_marginLeft="5dp"
                android:textColor="@color/white"
                android:text="@string/address" />
            <TextView
                android:id="@+id/ttrip"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_weight="0.8"
                android:textStyle="bold"
                android:layout_marginLeft="5dp"
                android:textSize="@dimen/repsubhead_fontport"
                android:textColor="@color/white"
                android:text="@string/tripno" />
            <TextView
                android:id="@+id/tcmdty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_weight="0.8"
                android:textStyle="bold"
                android:layout_marginLeft="5dp"
                android:textSize="@dimen/repsubhead_fontport"
                android:textColor="@color/white"
                android:text="@string/cmdty" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_weight="0.8"
                android:textStyle="bold"
                android:layout_marginLeft="5dp"
                android:textColor="@color/white"
                android:text="@string/remarkd" />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_weight="0.4"
                android:textStyle="bold"
                android:layout_marginLeft="5dp"

                android:textColor="@color/white"
                android:text="VECH#" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_weight="0.6"
                android:textStyle="bold"
                android:layout_marginLeft="5dp"

                android:textColor="@color/white"
                android:text="VIN#" />
            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginLeft="1dp"
                android:layout_weight="0.08"
                android:visibility="invisible"
                android:src="@drawable/cellular_icon"
                />
            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginLeft="5dp"
                android:layout_weight="0.08"
                android:visibility="invisible"
                android:src="@drawable/grey"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_weight="0.2"
                android:textStyle="bold"
                android:layout_marginLeft="5dp"

                android:textColor="@color/white"
                android:text="EDIT" />

        </TableRow>
    </TableLayout>
    <View
        android:id="@+id/view2"

        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/blue"></View>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/viewc">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/chatactivity_listview2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        >
    </androidx.recyclerview.widget.RecyclerView>
    </RelativeLayout>


    <View
        android:id="@+id/viewc"

        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/blue"></View>


        </LinearLayout>
    </ScrollView>





</LinearLayout>
    <!--</ScrollView>-->
